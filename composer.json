{
    "name": "manala/manalize",
    "description": "Lightweight binary providing vagrant stacks provisionned through manala ansible roles",
    "keywords": ["manala", "ansible", "vagrant", "docker"],
    "homepage": "https://github.com/manala/manalize",
    "license": "MIT",
    "authors": [
        {
            "name": "Manala",
            "email": "contact@manala.io",
            "homepage": "https://github.com/manala"
        }
    ],
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0",
        "symfony/console": "^2.8|^3.0",
        "symfony/filesystem": "^2.8|^3.0",
        "symfony/process": "^2.8|^3.0",
        "symfony/yaml": "^2.8|^3.0",
        "composer/semver": "^1.4",
        "nikic/iter": "^1.4",
        "guzzlehttp/guzzle": "^6.2",
        "elao/enum": "^0.4.0",
        "twig/twig": "~2.1"
    },
    "require-dev": {
        "symfony/console": "~3.2",
        "symfony/phpunit-bridge": "~2.8|~3.0",
        "friendsofphp/php-cs-fixer": "^2.0",
        "kherge/box": "^2.7"
    },
    "autoload": {
        "psr-4": { "Manala\\Manalize\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Manala\\Manalize\\Tests\\": "tests/" }
    },
    "bin": ["bin/manalize"]
}
